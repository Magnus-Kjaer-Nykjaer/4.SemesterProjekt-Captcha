@model Captcha.Shared.CaptchaLabelDto
@{
  ViewData["Title"] = "Admin panel";

}
<h1>@ViewData["Title"]</h1>

<p>Label og upload filer til data sættet</p>

<div>
  <form method="post" enctype="multipart/form-data">
    <input type="file" asp-for="File" accept="image/png, image/jpeg, image/jpg"/>

    <br/><br/>
    <img src="" alt="CaptchaImage" id="preview"/>
    <br/><br/>

    <label>Label billedet</label> <br/>

    <input type="text" asp-for="Name"/>

    <input type="submit" value="Submit biled"/>
  </form>
</div>

